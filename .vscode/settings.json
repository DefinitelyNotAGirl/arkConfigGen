    "psi-header.config": {
        "forceToTop": true,
        "blankLinesAfter": 1,
        "license": "Custom"
    },
    "psi-header.changes-tracking": {
        "isActive": true,
        "include": [],
        "exclude": [
            "markdown",
            "json",
            "jsonc"
        ],
        "excludeGlob": [
            "out/**",
            "bin/**"
        ],
        "autoHeader": "manualSave"
    },
    "psi-header.lang-config": [
        {
            "language": "python",
            "begin": "###",
            "prefix": "# ",
            "end": "###",
            "beforeHeader": [
                "#!/usr/bin/env python3",
                "# -*- coding:utf-8 -*-"
            ]
        },
        {
            "language": "makefile",
            "begin": "#",
            "prefix": "# ",
            "end": "#",
        },
        {
            "language": "javascript",
            "begin": "/**",
            "prefix": " * ",
            "end": " */",
        },
        {
            "language": ".h",
            "begin": "/**",
            "prefix": " * ",
            "end": " */",
            "blankLinesAfter": 0,
            "afterHeader": [
                "#pragma once"
            ]
        },
        {
            "language": ".ini",
            "begin": "#",
            "prefix": "# ",
            "end": "#",
            "blankLinesAfter": 0,
            "afterHeader": []
        },
        {
            "language": ".cpp",
            "begin": "/**",
            "prefix": " * ",
            "end": " */",
            "blankLinesAfter": 0,
            "afterHeader": [
                "#pragma once"
            ]
        },
        {
            "language": ".asm",
            "begin": ";",
            "prefix": "; ",
            "end": ";",
        },
        {
            "language": "typescript",
            "mapTo": "javascript"
        },
        {
            "language": ".hpp",
            "mapTo": ".h"
        },
        {
            "language": ".hxx",
            "mapTo": ".h"
        },
        {
            "language": ".c",
            "mapTo": ".cpp"
        }
    ],
